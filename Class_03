def selectionAsc(arr:Array[Int]):Array[Int]={
  var size:Int = arr.length
  var minIndex:Int = 0 
  var holder = 0
  for (i <- 0 until size){
    for (j <- i+1 until size){
      if (arr(minIndex) > arr(j)){   
        holder = arr(j)
        arr(j) = arr(minIndex)
        arr(minIndex) = holder
        min = j
      }
    }    
  }
}


def selectionAsc(arr:Array[Int]):Array[Int]={
  var size:Int = arr.length
  var minIndex:Int = 0 
  var holder = 0
  for (i <- 0 until size){
    for (j <- i until size){
      minIndex = i
      if (arr(minIndex) > arr(j)){  
        minIndex = j
      }
    holder = arr(i)
    arr(i) = arr(minIndex)
    arr(minIndex) = holder     
    }    
  }
  return arr
}